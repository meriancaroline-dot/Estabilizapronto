{
  "expo": {
    "name": "Estabiliza",
    "slug": "estabiliza",
    "scheme": "estabiliza",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seunome.estabiliza"
    },
    "android": {
      "package": "com.seunome.estabiliza",
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.POST_NOTIFICATIONS"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#B4A5D9",
      "androidMode": "default",
      "androidCollapsedTitle": "Estabiliza"
    },
    "plugins": [
      "expo-notifications",
      "expo-asset",
      "expo-font",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "6b429ee4-4976-41c3-ad94-98edc531f2fe"
      }
    }
  }
}
